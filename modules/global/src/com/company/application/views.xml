<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<views xmlns="http://schemas.haulmont.com/cuba/view.xsd">
    <view entity="application_Retailer" name="retailer-view" extends="_local">
        <property name="user" view="_minimal"/>
        <property name="address" view="_minimal"/>
    </view>
    <view entity="application_Product" name="product-view" extends="_local">
        <property name="retailer" view="_minimal">
            <property name="name"/>
            <property name="user" view="_minimal"/>
            <property name="address" view="_minimal"/>
        </property>
    </view>
    <view entity="application_StoreChain" name="storeChain-view" extends="_local"/>
    <view entity="application_Purchase" name="purchase-view" extends="_local">
        <property name="store" view="_minimal"/>
        <property name="products" view="_minimal">
            <property name="product" view="_minimal">
                <property name="name"/>
            </property>
            <property name="price"/>
            <property name="count"/>
        </property>
    </view>
    <view entity="application_Store" name="store-view" extends="_local">
        <property name="storeChain" view="_minimal">
        </property>
        <property name="address" view="_minimal">
        </property>
        <property name="storeProducts" view="_minimal">
            <property name="product" view="_minimal">
                <property name="priceFromRetailer"/>
            </property>
            <property name="price"/>
            <property name="count"/>
        </property>
    </view>
    <view entity="application_StoreProduct" name="storeProduct-view" extends="_local">
        <property name="store" view="_minimal"/>
        <property name="product" view="_minimal"/>
    </view>
</views>